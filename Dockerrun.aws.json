{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "eSignPDF",
        "image": "thiagosvalentim/eideasy-test:latest",
        "essential": true,
        "memory": 2048,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80
          }
        ],
        "environment": [
          {
            "name": "ENV",
            "value": "production"
          }
        ],
        "mountPoints": [
          {
            "sourceVolume": "backend-volume",
            "containerPath": "/var/www"
          },
          {
            "sourceVolume": "nginx-config",
            "containerPath": "/etc/nginx/nginx.conf"
          },
          {
            "sourceVolume": "nginx-conf",
            "containerPath": "/etc/nginx/conf.d/default.conf"
          }
        ]
      }
    ],
    "volumes": [
      {
        "name": "backend-volume",
        "host": {
          "sourcePath": "/var/www"
        }
      },
      {
        "name": "nginx-config",
        "host": {
          "sourcePath": "/etc/nginx/nginx.conf"
        }
      },
      {
        "name": "nginx-conf",
        "host": {
          "sourcePath": "/etc/nginx/conf.d/default.conf"
        }
      }
    ]
  }
  